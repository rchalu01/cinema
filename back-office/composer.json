{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "symfony/web-server-bundle": "^4.4.17",
        "symfony/debug": "^4.4",
        "symfony/maker-bundle": "^1.25",
        "symfony/phpunit-bridge": "^5.2"
    },
    "require": {
        "symfony/flex": "^1.11",
        "symfony/routing": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/yaml": "^5.2",
        "symfony/annotations-pack": "^1.0",
        "symfony/form": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/security-csrf": "^5.2",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
